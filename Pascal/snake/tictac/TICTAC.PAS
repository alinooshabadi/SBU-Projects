              gotoxy(61,45);
              write('Äþokay!þÄÄÄÄÄÄ');
              delay(messagetime div 3);
              gotoxy(61,45);
              write('              ');
              move:=true;
            end;
          if ((player='x') and (move=true) and (used=false)) then
            begin
              putX(box);
              grid[box]:='x';
            end;
          if ((player='o') and (move=true) and (used=false)) then
            begin
              putO(box);
              grid[box]:='o';
            end;
        end;
      'F' : {FORFIET}
        begin
          quit:=true;
          loser:=true;
        end;
      'R' : {RESTART}
        begin
          gotoxy(1,49);
          write('restarting!');
          delay(messagetime);
          clrscr;
          goto 1;
        end;
      'H' : {HELP}
        begin
          help:=true;
          document;
        end;
      'Q' : {QUIT}
        begin
          quit:=true;
        end;
      else
        begin
          gotoxy(61,43);
          write('Äþwrong key!þÄ');
          delay(messagetime div 3);
          gotoxy(61,43);
          write('             ');
          goto 2;
        end;
    end;
    checkwin(grid);
    checkdud(grid);
    if win=true then
      begin
        if winner='x' then
          begin
            gotoxy(1,49);
            write('the winner is X! ');
          end;
        if winner='o' then
          begin
            gotoxy(1,49);
            write('the winner is O! ');
          end;
        delay(messagetime*2);
      end;
    if dud=true then
      begin
        gotoxy(1,49);
        write('nobody one that game, restarting!');
        delay(messagetime);
        clrscr;
        goto 1;
      end;
  until (quit=true) or (win=true);
  gotoxy(1,49);
  if (not(loser=true) and not(quit=true)) then
    begin
      writeln('would you like to play again? [y/n] : ');
      again:=readkey;
      case upcase(again) of
        'Y': begin
               goto 1;
               writeln('okay!');
               delay(messagetime);
             end;
        'N': writeln('play again some other time! ');
      end;
    end;
  if loser=true then
    write('You are a loser!  You quiter! ');
  if (quit=true) or (loser=true)then
    writeln('Have a nice day!');
  delay(messagetime);
  randomshapes;
  clrscr;
  write('Game Over! '); displaytime;
end.
